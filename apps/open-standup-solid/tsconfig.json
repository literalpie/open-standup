{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "types": ["solid-start/env", "vite-plugin-solid-svg/types"],
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"],
      "open-standup-shared": ["../../libs/open-standup-shared/src"]

    }
  }
}
